<br>
<div class="container py-5">
    <div class="row">
      <div class="col">
        <div class="card-new rounded p-3">
        <h5> Create a new list </h5>
          <%= simple_form_for(@list) do |f| %>
            <%= f.input :name, placeholder: 'Drama' %>
            <%= f.input :photo, as: :file %>
            <%= f.submit class: "btn btn-outline-light" %>
          <% end %>
      </div>
    </div>
  </div>
</div>
